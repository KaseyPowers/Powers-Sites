server {
	server_name thefamilypowers.com www.thefamilypowers.com;
	root /home/kasey/workspace/powers-sites/build/thefamilypowers;
	location / {
		try_files $uri /index.html;
	}
	location /wedding {
		root /home/kasey/workspace/powers-sites/build/wedding;
		try_files $uri /index.html;
	}
	location /commons.js {
		root /home/kasey/workspace/powers-sites/build;
	}
	location /fonts {
		root /home/kasey/workspace/powers-sites/build/fonts;
	}
}

